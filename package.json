{
  "name": "skeleton",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "npm run prod",
    "build": "npm-run-all clean transpile",
    "server": "node ./dist-server/bin/www",
    "dev": "NODE_ENV=development npm-run-all build server",
    "prod": "NODE_ENV=production npm-run-all build server",
    "transpile": "babel ./server --out-dir dist-server",
    "clean": "rimraf dist-server",
    "watch:dev": "nodemon",
    "test": "jest",
    "eslint-check": "eslint --print-config ./server/app.js | eslint-config-prettier-check",
    "build-webpack": "webpack --mode production",
    "start-webpack": "webpack-dev-server --open --mode development",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "dev-ts": "nodemon dist/index.js",
    "dev2-ts": "nodemon --exec ts-node src/index.ts",
    "create:migration": "mikro-orm migration:create",
    "start-ts": "node dist/index.js",
    "start2-ts": "ts-node src/index.ts"
  },
  "dependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
    "@mikro-orm/cli": "^4.0.7",
    "@mikro-orm/core": "^4.0.7",
    "@mikro-orm/migrations": "^4.0.7",
    "@mikro-orm/postgresql": "^4.0.7",
    "apollo-server-express": "^2.18.2",
    "argon2": "^0.27.0",
    "babel-loader": "^8.1.0",
    "chokidar": "^3.4.0",
    "connect": "^3.7.0",
    "connect-livereload": "^0.6.1",
    "connect-redis": "^5.0.0",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "ejs": "~2.6.1",
    "express": "~4.16.1",
    "express-session": "^1.17.1",
    "graphql": "^15.3.0",
    "html-loader": "^1.3.2",
    "html-webpack-plugin": "^4.5.0",
    "http-errors": "~1.6.3",
    "livereload": "^0.9.1",
    "morgan": "~1.9.1",
    "nodemon": "^2.0.4",
    "npm-run-all": "^4.1.5",
    "pg": "^8.4.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "regis": "^0.0.0",
    "rimraf": "^3.0.2",
    "server-static": "^2.0.5",
    "socket.io": "^2.3.0",
    "tiny-lr": "^1.1.1",
    "type-graphql": "^1.0.0",
    "vanilla-router": "^1.2.8",
    "webpack-dev-server": "^3.11.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.8",
    "@types/express-session": "^1.17.0",
    "@types/node": "^14.11.8",
    "@typescript-eslint/eslint-plugin": "^4.4.0",
    "@typescript-eslint/parser": "^4.4.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.11.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^2.5.0",
    "jest": "^25.1.0",
    "prettier": "^2.0.5",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12"
  },
  "nodemonConfig": {},
  "jest": {
    "testEnvironment": "node"
  }
